<wpml-config>
	<custom-types>
		<custom-type translate="1">portfolio</custom-type>
	</custom-types>
	<taxonomies>
		<taxonomy translate="1">portfolio_cat</taxonomy>
		<taxonomy translate="1">portfolio_client</taxonomy>
		<taxonomy translate="1">portfolio_tag</taxonomy>
	</taxonomies>
	<custom-fields>
		<custom-field action="translate">_edit_last</custom-field>
		<custom-field action="translate">_custom_page_options</custom-field>
		<custom-field action="translate">_custom_wc_options</custom-field>
	</custom-fields>
	<admin-texts>
		<key name="_cs_options">
			<key name="header-top-left" />
			<key name="header-top-center" />
			<key name="header-top-right" />
			<key name="footer-copyright" />
			<key name="wc-page-title" />
			<key name="wc-page-desc" />
			<key name="wc-single-shipping-return" />
			<key name="wc-single-size-guide" />
			<key name="wc-extra-content" />
			<key name="wc-minicart-content" />
			<key name="wc-cart-content" />
			<key name="wc-checkout-content" />
			<key name="maintenance-title" />
			<key name="maintenance-message" />
			<key name="maintenance-date" />
			<key name="maintenance-month" />
			<key name="maintenance-year" />
		</key>
	</admin-texts>
	<shortcodes>
		<shortcode>
			<tag>claue_addons_google_maps</tag>
			 <attributes>
				<attribute>infowindow</attribute>
			</attributes>
		</shortcode>
		<shortcode>
			<tag>claue_addons_promotion</tag>
			 <attributes>
				<attribute>text_1</attribute>
				<attribute>text_2</attribute>
				<attribute>text_3</attribute>
			</attributes>
		</shortcode>
		<shortcode>
			<tag>claue_addons_service</tag>
			 <attributes>
				<attribute>title</attribute>
				<attribute>entry</attribute>
			</attributes>
		</shortcode>
		<shortcode>
			<tag>claue_addons_banner</tag>
			 <attributes>
				<attribute>text</attribute>
				<attribute type="link">link</attribute>
			</attributes>
		</shortcode>
		<shortcode>
			<tag>claue_addons_member </tag>
			 <attributes>
				<attribute>name</attribute>
				<attribute>job</attribute>
			</attributes>
		</shortcode>
	</shortcodes>
</wpml-config>